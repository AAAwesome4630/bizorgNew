<p id="notice"><%= notice %></p>

<h1>Listing Org Offers</h1>

<table>
  <thead>
    <tr>
      <th>Offer</th>
      <th>Orginization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @org_offers.each do |org_offer| %>
      <tr>
        <td><%= org_offer.offer %></td>
        <td><%= org_offer.orginization %></td>
        <td><%= link_to 'Show', org_offer %></td>
        <td><%= link_to 'Edit', edit_org_offer_path(org_offer) %></td>
        <td><%= link_to 'Destroy', org_offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Org offer', new_org_offer_path %>
